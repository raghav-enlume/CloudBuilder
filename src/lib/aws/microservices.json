{
    "us-east-1": {
        "vpcs": [
            {
                "VpcId": "vpc-micro-001",
                "CidrBlock": "10.1.0.0/16",
                "State": "available"
            }
        ],
        "subnets": [
            {
                "SubnetId": "subnet-public-a",
                "VpcId": "vpc-micro-001",
                "CidrBlock": "10.1.1.0/24",
                "AvailabilityZone": "us-east-1a",
                "Type": "public"
            },
            {
                "SubnetId": "subnet-public-b",
                "VpcId": "vpc-micro-001",
                "CidrBlock": "10.1.2.0/24",
                "AvailabilityZone": "us-east-1b",
                "Type": "public"
            },
            {
                "SubnetId": "subnet-private-a",
                "VpcId": "vpc-micro-001",
                "CidrBlock": "10.1.11.0/24",
                "AvailabilityZone": "us-east-1a",
                "Type": "private"
            },
            {
                "SubnetId": "subnet-private-b",
                "VpcId": "vpc-micro-001",
                "CidrBlock": "10.1.12.0/24",
                "AvailabilityZone": "us-east-1b",
                "Type": "private"
            }
        ],
        "internet_gateways": [
            {
                "InternetGatewayId": "igw-micro-001",
                "VpcId": "vpc-micro-001"
            }
        ],
        "nat_gateways": [
            {
                "NatGatewayId": "nat-micro-001",
                "SubnetId": "subnet-public-a"
            }
        ],
        "security_groups": [
            {
                "GroupId": "sg-alb",
                "Description": "ALB public access",
                "InboundRules": [
                    {
                        "Protocol": "tcp",
                        "Port": 80,
                        "Source": "0.0.0.0/0"
                    }
                ]
            },
            {
                "GroupId": "sg-service",
                "Description": "Microservices access from ALB",
                "InboundRules": [
                    {
                        "Protocol": "tcp",
                        "Port": 8080,
                        "SourceSecurityGroup": "sg-alb"
                    }
                ]
            }
        ],
        "load_balancers": [
            {
                "LoadBalancerName": "microservices-alb",
                "Type": "application",
                "Scheme": "internet-facing",
                "Subnets": [
                    "subnet-public-a",
                    "subnet-public-b"
                ],
                "SecurityGroups": [
                    "sg-alb"
                ]
            }
        ],
        "ecs": {
            "ClusterName": "microservices-cluster",
            "LaunchType": "FARGATE",
            "Services": [
                {
                    "ServiceName": "user-service",
                    "DesiredCount": 2,
                    "Container": {
                        "Image": "ecr-user-service",
                        "Port": 8080
                    },
                    "Subnets": [
                        "subnet-private-a",
                        "subnet-private-b"
                    ],
                    "SecurityGroup": "sg-service",
                    "ServiceDiscovery": true,
                    "Database": "users-db"
                },
                {
                    "ServiceName": "order-service",
                    "DesiredCount": 2,
                    "Container": {
                        "Image": "ecr-order-service",
                        "Port": 8080
                    },
                    "Subnets": [
                        "subnet-private-a",
                        "subnet-private-b"
                    ],
                    "SecurityGroup": "sg-service",
                    "ServiceDiscovery": true,
                    "Database": "orders-table"
                }
            ]
        },
        "ecr_repositories": [
            {
                "RepositoryName": "ecr-user-service"
            },
            {
                "RepositoryName": "ecr-order-service"
            }
        ],
        "service_discovery": {
            "Namespace": "services.local",
            "Type": "AWS Cloud Map"
        },
        "databases": {
            "rds_instances": [
                {
                    "DBName": "users-db",
                    "Engine": "postgresql",
                    "MultiAZ": true,
                    "SecurityGroup": "sg-service"
                }
            ],
            "dynamodb_tables": [
                {
                    "TableName": "orders-table",
                    "BillingMode": "PAY_PER_REQUEST"
                }
            ]
        },
        "messaging": {
            "sqs_queues": [
                {
                    "QueueName": "order-events-queue"
                }
            ],
            "sns_topics": [
                {
                    "TopicName": "order-events-topic"
                }
            ]
        },
        "cloudwatch": {
            "Logs": true,
            "Metrics": true
        }
    }
}